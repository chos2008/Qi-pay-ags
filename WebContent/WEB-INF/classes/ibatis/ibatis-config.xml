<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
  "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.chos.payment.Ta" alias="Ta"/>
		<typeAlias type="com.chos.payment.TaA" alias="TaA"/>
		<typeAlias type="com.chos.payment.transaction.Transaction" alias="Transaction"/>
		<typeAlias type="com.chos.payment.transaction.CallbackEndpoint" alias="CallbackEndpoint"/>
		<typeAlias type="com.chos.payment.transaction.log.TransactionLog" alias="TransactionLog"/>
	</typeAliases>
		

	<mappers>
		<mapper resource="ibatis/ta.xml" />
		<mapper resource="ibatis/TaA.xml" />
		<mapper resource="ibatis/Transaction.xml" />
	</mappers>
	
</configuration>
